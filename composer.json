{
    "name": "mohamedhekal/arabic-laravel-mpdf",
    "description": "Laravel Mpdf: Arabic Customized for Using Mpdf in Laravel to generate Pdfs.",
    "keywords": [
        "mpdf",
        "pdf",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "mpdf/mpdf": "^8.0|^8.1"
    },
    "autoload": {
        "psr-4": {
            "MohamedHekal\\ArabicLaravelMpdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohamedHekal\\ArabicLaravelMpdf\\ArabicLaravelMpdfServiceProvider"
            ],
            "aliases": {
                "PDF": "MohamedHekal\\ArabicLaravelMpdf\\Facades\\ArabicLaravelMpdf"
            }
        }
    }
}